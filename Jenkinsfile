pipeline {
  agent any
  stages {
    stage('pre-build') {
      steps {
        echo 'pre-build'
      }
    }

    stage('build') {
      steps {
        echo 'build'
      }
    }

    stage('deploy') {
      steps {
        echo 'deploy'
      }
    }

    stage('test') {
      steps {
        echo 'test'
      }
    }

    stage('deploy to prod') {
      steps {
        echo 'deploy to prod'
      }
    }

  }
}